<UserControl x:Class="WPF_Tranning.View.FileCopyV"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dxsps="http://schemas.devexpress.com/winfx/2008/xaml/spreadsheet"  
             mc:Ignorable="d"
             xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
             Height="831" 
             Width="1266"
             xmlns:local="clr-namespace:WPF_Tranning.ModelAndView">

    <UserControl.DataContext>
        <local:FileCopyVM/>
    </UserControl.DataContext>

    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="230" Margin="100,155,0,0" VerticalAlignment="Top" Width="1055"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="200" Margin="100,435,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="1055" AllowDrop="True"/>


    </Grid>
</UserControl>
